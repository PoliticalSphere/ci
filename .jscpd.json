{
  "configVersion": "1.0",
  "minTokens": 60,
  "threshold": 1.25,
  "reporters": ["console", "markdown"],
  "output": "reports/jscpd-report",
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/html/**",
    "**/logs/**",
    "**/.jscpd-report/**",
    "**/reports/**",
    "**/.github/workflows/**",
    "**/package-lock.json",
    "**/pnpm-lock.yaml",
    "**/yarn.lock"
  ],
  "files": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.mjs",
    "src/**/*.cjs",
    "src/**/*.mts"
  ],
  "excludes": [],
  "languages": {
    "javascript": ["js", "jsx", "mjs", "cjs"],
    "typescript": ["ts", "tsx", "mts"]
  },
  "blame": false
}
